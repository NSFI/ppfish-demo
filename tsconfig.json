{
    "compilerOptions": {
    "outDir": "./dist/",
        "sourceMap": false,
        "experimentalDecorators": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "lib": [
        "es5",
        "dom",
        "dom.iterable",
        "es2016"
    ],
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "baseUrl": "./src/"
},
    "include": [
    "src/**/*"
],
    "exclude": [
    "node_modules"
]
}